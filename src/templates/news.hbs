{{#each this}}
<div class="card">
    <div class="card-img-top">
        <img src="{{urlToImage}}" alt="{{source.name}}">
    </div>
    <div class="card-body">
        <h2 class="card-title">Назва статті: {{source.name}}</h2>
        <p class="card-text">Опис: {{description}}</p>

    </div>
</div>
{{/each}}